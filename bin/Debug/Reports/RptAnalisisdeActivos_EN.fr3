;&#13;&#10;  if (ParamValue('ValRema') &#60;&#62; 'nil') then&#13;&#10;     AdoQuery1.Sql.add(ParamValue('ValRema' ));&#13;&#10;&#13;&#10;  AdoQuery1.Sql.add(' Order by u_ItmGrpCD, u_actcode ' );&#13;&#10;&#13;&#10;  if (ParamValue('GrpLevel') = 'D') then begin&#13;&#10;     ;&#13;&#10;  end&#13;&#10;  else if (ParamValue('GrpLevel') = 'G') then begin&#13;&#10;     PageHeader1.Visible := false;&#13;&#10;     MasterData1.Visible := false;&#13;&#10;  end;&#13;&#10;end." StoreInDFM="False" OnStartReport="frxReport1OnStartReport" PropData="044C65667403900003546F70027808446174617365747301010C2C00000020446174615365743D2241444F5175657279312220446174615365744E616D653D2241444F517565727931220000095661726961626C65730100055374796C650100">
